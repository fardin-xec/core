{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "esnext",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": ".",
    "lib": ["es2015", "dom"],
    "skipLibCheck": true
  },
  "include": ["expose/**/*"],
  "exclude": ["node_modules"]
}
